<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="robor.wildfiremapper">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="robor.wildfiremapper.WildfireMapperApplication">

        <service android:name=".service.bluetooth.mldp.MLDPConnectionService" />
        <service android:name=".service.SyncService" />
        <service android:name=".service.bluetooth.DeviceScanService" />
        <service android:name=".service.bluetooth.mldp.MLDPDataReceiverService" />
    </application>

</manifest>